{
  "version": 2,
  "rootDirectory": "frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_WS_URL": "wss://alpha-gen-backend.railway.app/ws/market-data",
    "NEXT_PUBLIC_API_URL": "https://alpha-gen-backend.railway.app"
  }
}
