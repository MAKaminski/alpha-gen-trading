# Polygon
POLYGON_API_KEY=your_polygon_api_key
POLYGON_S3_ACCESS_KEY=your_polygon_s3_access_key
POLYGON_S3_SECRET_KEY=your_polygon_s3_secret_key
POLYGON_S3_ENDPOINT=https://files.polygon.io
POLYGON_EQUITY_TICKER=QQQ
POLYGON_OPTIONS_UNDERLYING=QQQ
POLYGON_STOCK_WS_URL=wss://socket.polygon.io/stocks
POLYGON_OPTIONS_WS_URL=wss://socket.polygon.io/options

# Schwab
SCHWAB_API_KEY=your_schwab_api_key
SCHWAB_API_SECRET=your_schwab_app_secret
SCHWAB_CALLBACK_URL=https://127.0.0.1:8182/
SCHWAB_TOKEN_PATH=./schwab_token.json
SCHWAB_ACCOUNT_ID=your_account_id
SCHWAB_BASE_URL=https://api.schwab.com
SCHWAB_PAPER=true

# Database
DATABASE_URL=sqlite+aiosqlite:///./alpha_gen.db

# Risk parameters
RISK_STOP_LOSS_MULTIPLE=2.0
RISK_TAKE_PROFIT_MULTIPLE=0.5
RISK_MAX_POSITION_SIZE=25
RISK_TRADING_CAPITAL=5000000

# Feature flags
FEATURE_ENABLE_CHART=false
